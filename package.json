{
  "name": "nextarter-chakra",
  "version": "0.1.0",
  "private": true,
  "author": "sozonome",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "turbo": "pnpm dlx turbo@2.0.9",
    "build:turbo": "pnpm turbo run build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm format",
    "type-check": "tsc --noEmit",
    "check:turbo": "pnpm turbo lint type-check",
    "test:e2e": "playwright test",
    "format": "prettier --write src",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky"
  },
  "dependencies": {
    "@chakra-ui/next-js": "^2.2.0",
    "@chakra-ui/react": "^2.10.4",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "formik": "^2.4.6",
    "framer-motion": "^11.15.0",
    "locomotive-scroll": "5.0.0-beta.21",
    "next": "^14.2.5",
    "next-pwa": "^5.6.0",
    "nextjs-toploader": "^3.7.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@commitlint/cz-commitlint": "19.2.0",
    "@playwright/test": "^1.45.3",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-sznm": "^2.0.3",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.3",
    "lint-staged": "^15.2.7",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.4.2",
    "standard-version": "^9.5.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=20.14.x",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.1.2"
}
